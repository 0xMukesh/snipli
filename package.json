{
  "name": "gistly",
  "version": "0.0.0",
  "description": "âœ¨ A command-line interface for quickly sharing code snippets for your local files",
  "author": "Kira272921",
  "bin": {
    "gistly": "./bin/run"
  },
  "homepage": "https://github.com/Kira272921/gistly",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Kira272921/gistly",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "figchalk": "^1.0.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/node": "^16.11.27",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "gistly",
    "dirname": "gistly",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Kira272921/gistly/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
