{
  "name": "snipli",
  "version": "1.1.22",
  "description": "âœ¨ A command-line interface for quickly sharing code snippets for your local files",
  "author": "Kira272921",
  "bin": {
    "snipli": "./bin/run"
  },
  "homepage": "https://github.com/Kira272921/snipli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Kira272921/snipli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "@octokit/core": "^4.0.0",
    "axios": "^0.27.2",
    "chalk": "4.1.2",
    "express": "^4.17.3",
    "figchalk": "^1.0.3",
    "inquirer": "^9.0.0",
    "object-path": "^0.11.8",
    "open": "^8.4.0",
    "ora": "5.4.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/test": "2.1.0",
    "@types/express": "4.17.13",
    "@types/inquirer": "8.2.1",
    "@types/node": "16.11.41",
    "@types/object-path": "0.11.1",
    "@types/shelljs": "0.8.11",
    "eslint": "8.18.0",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.2",
    "globby": "13.1.2",
    "oclif": "3.0.1",
    "shx": "0.3.4",
    "ts-node": "10.8.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "oclif": {
    "bin": "snipli",
    "dirname": "snipli",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest",
    "version": "oclif readme && git add README.md",
    "watch": "tsc -b -w"
  },
  "resolutions": {
    "figchalk/**/yargs/**/yargs-parser": ">=13.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Kira272921/snipli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
